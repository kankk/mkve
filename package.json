{
  "name": "mkve",
  "version": "1.0.0",
  "description": "`Vue3` + `TypeScript` + `Element-Plus` 搭建的项目",
  "scripts": {
    "lint:commands": "cd commands && eslint --ext .js,.ts . --fix && cd ..",
    "lint:client": "cd client && eslint --ext .js,.ts,.vue ./src --fix && cd ..",
    "lint:server": "cd commands && eslint --ext .js,.ts . --fix && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kankk/mkve.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kankk/mkve/issues"
  },
  "homepage": "https://github.com/kankk/mkve#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitlint": "^11.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "client/src/**/*.{js,ts,vue}": [
      "npm run lint:client"
    ]
  }
}
